<!--index.wxml-->
<view class="container">
<mp-searchbar placeholder="请输入搜索内容"  search="{{search}}"></mp-searchbar>
<swiper wx:if="{{banners.length > 0}}" indicator-dots="true" autoplay="true" circular="true" previous-margin="40rpx" next-margin="40rpx">
<block  wx:for="{{banners}}"  wx:key="*this">
  <swiper-item>
  <image src="{{item.imagePath}}" class="banner_image" mode="widthFix"></image>
  </swiper-item>
</block>

</swiper>
</view>
