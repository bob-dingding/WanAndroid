<?xml version="1.0" encoding="utf-8"?>
<resources>

<declare-styleable name="web_progress">
<!--    进度条的宽度-->
    <attr name="progress_border_width" format="dimension"/>
<!--    进度条的颜色-->
    <attr name="progress_border_color" format="color"/>
    <!--    填充的颜色-->
    <attr name="progress_bg_color" format="color"/>
<!--    进度条的起始位置-->
    <attr name="progress_border_startPosition" format="enum">
       <enum name="left" value="1" />
       <enum name="top" value="2" />
       <enum name="right" value="3" />
       <enum name="bottom" value="4" />
    </attr>
</declare-styleable>
</resources>